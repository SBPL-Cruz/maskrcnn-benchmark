cd /data/aditya/fb_mask_rcnn/maskrcnn-benchmark
echo "Using $1 GPUs"
export CONFIG="configs/ycb_mask/e2e_mask_rcnn_R_101_FPN_$1x_cocostyle.yaml"
echo "CONFIG : $CONFIG"
export NGPUS=$1
python -m \
    torch.distributed.launch \
    --nproc_per_node=$NGPUS tools/train_net.py \
    --use-tensorboard \
    --config $CONFIG |& tee log
